<!DOCTYPE html><html lang="fr-fr">    <head>        <meta charset="utf-8">        <!--<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">-->        <title>{% block title %}{{title_website}}{% endblock %}</title>        <meta name="description" content="">        <meta name="author" content="">                <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">        <!-- © Bootstrap -->        <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">        <!-- THEME -->        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('css/font-awesome.min.css') }}">        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('css/smartadmin-production.css') }}">        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('css/smartadmin-skins.css') }}">        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('css/ib.css') }}">        <!-- FAVICONS -->        <link rel="shortcut icon" href="img/favicon/favicon.ico" type="image/x-icon">        <link rel="icon" href="img/favicon/favicon.ico" type="image/x-icon">        <!-- GOOGLE FONT -->        <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,700italic,300,400,700">        {% block stylesheets %}        {% endblock %}        {% block js_head %}            <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>            <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>        {% endblock %}    </head>    <body class="container smart-style-2 {% if not menu_transversale_open|default(false) %}non-transvervale{% endif %}" >        {% include ':Include:barre.html.twig' %}        <!-- panel de droite : Navigation -->        {% block menu_transversale %}        {% endblock %}        <!-- FIN NAVIGATION -->        <!-- PANEL PRINCIPAL -->        <div id="main" role="main">            <!-- RIBBON -->            <div id="ribbon">                <!-- breadcrumb -->                <ol class="breadcrumb">                    <i class="fa fa-cog fa-spin"></i>                </ol>                <!-- fin breadcrumb -->                <span class="pull-right hide" id="app_loader">                    <i class="fa fa-cog fa-spin"></i> Chargement...                </span>            </div>            <!-- FIN RIBBON -->            <!-- CONTENUE PRINCIPAL -->            <div id="content" class="ib_page">                {% block Body_content %}                {% endblock %}            </div>            <!-- CONTENUE PRINCIPAL -->        </div>        <!-- FIN PANEL PRINCIPAL -->        <!-- #PAGE FOOTER -->        <div class="page-footer">            <div class="row">                <div class="col-xs-12 col-sm-6">                    © {{title_website}} {{ "now"|date("Y") }}                </div>                <div class="col-xs-6 col-sm-6 text-right hidden-xs">                    {{ render(url("ib_page_get_menu_footer")) }}                    <!-- fin div-->                </div>                <!-- fin col -->            </div>            <!-- fin row -->        </div>        <!-- fin FOOTER -->        <!--================================================== -->        <!-- Link to Google CDN's jQuery + jQueryUI + Bootstrap; fall back to local -->        <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>        <script>            if (!window.jQuery) {                document.write("<script src='{{ asset('js/libs/jquery-2.0.2.min.js') }}'><\/script>");            }        </script>        <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.11.0/jquery-ui.min.js"></script>        <script>            if (!window.jQuery.ui) {                document.write("<script src='{{ asset('js/libs/jquery-ui-1.10.3.min.js') }}'><\/script>");            }        </script>        <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>        <!-- JS -->        <script src="{{ asset('js/notification/SmartNotification.min.js') }}"></script>        <script src="{{ asset('js/smartwidgets/jarvis.widget.min.js') }}"></script>        {% javascripts '@IBSchemaBundle/Resources/assets/js/jquery.history.js' %}            <script type="text/javascript" src="{{ asset_url }}"></script>        {% endjavascripts %}                {% javascripts '@IBSchemaBundle/Resources/assets/js/ib_tools.js' '@IBSchemaBundle/Resources/assets/js/ib_init.js' %}            <script type="text/javascript" src="{{ asset_url }}"></script>        {% endjavascripts %}        <!--[if IE 7]>            <h1>Votre navigateur est trop ancien, veuillez le mettre à jour sur www.microsoft.com/download</h1>        <![endif]-->        <script type="text/javascript">            ib_api_pathInfo = "{{app.request.requestUri}}";            pageSetUp();        </script>        {% block js_bottom %}        {% endblock %}    </body></html>